<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:dataGrid="clr-namespace:DataDeveloper.DataGrid;assembly=DataDeveloper.DataGrid"
             xmlns:viewModels="clr-namespace:DataDeveloper.ViewModels"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="DataDeveloper.Views.ResultView">
    <Design.DataContext>
        <viewModels:EditorDocumentViewModel />
    </Design.DataContext>
   
    <dataGrid:SkiaDataGridControl Background="CadetBlue"
        Headers="{Binding MyHeaders}"
        Data="{Binding MyData}" />
</UserControl>
